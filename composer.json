{
    "name": "t3md/blackboard",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Michael Lang",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "T3md\\Blackboard\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3md\\Blackboard\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/blackboard": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "blackboard"
        }
    }
}
